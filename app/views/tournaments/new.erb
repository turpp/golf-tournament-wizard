<%= form_for @tournament do |f| %>
<%= f.label :name %>
<%= f.text_field :name %>
<%= f.label :date %>
<%= f.date_field :date %>
<%= f.label :entry_fee %>
<%= f.number_field :entry_fee %>
<%= f.label :user_id %>
<%= f.number_field :user_id %>
<%= f.label :players_on_team %>
<%= f.number_field :players_on_team%>
<br>
<br>

<%= f.submit %>
<% end %>