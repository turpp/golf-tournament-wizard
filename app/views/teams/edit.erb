<%= form_for @team do |f| %>
<%= f.label :final_score %>
<%= f.number_field :final_score %>
<%= f.label :divison %>
<%= f.text_field :divison %>
<%= f.label :tournament_id %>
<%= f.number_field :tournament_id %>
<br>
<%= f.fields_for :players_teams do |pt| %>
<%= pt.label "Team Member:" %>
<%= pt.collection_select :player_id, Player.all, :id, :name %>
<% end %>





<br>
 <%= f.submit %>
 <% end %>
<%= button_to "Delete Team", team_path(@team), method: :delete %>