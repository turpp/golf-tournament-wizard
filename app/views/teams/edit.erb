<%= form_for @team do |f| %>
<%= f.label :final_score %>
<%= f.number_field :final_score %>
<%= f.label :divison %>
<%= f.text_field :divison %>
<%= f.label :tournament_id %>
<%= f.number_field :tournament_id %>
<br>
<% @team.players.each do |p| %>
<%= label_tag "Current player: #{p.name}" %>
<%= collection_select "player_id", p.id, Player.all, :id, :name %>
<% end %>
 







 <%= f.submit %>
 <% end %>
